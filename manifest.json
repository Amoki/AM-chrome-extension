{
  "manifest_version": 2,

  "name": "Amoki-Muisque",
  "description": "Remote for Amoki-Musique",
  "version": "0.1",

  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/",
    "storage"
  ]
}